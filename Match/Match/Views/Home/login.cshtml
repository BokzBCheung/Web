			<script type="text/javascript">				
			    $("#submit").click(function () {
			        var username = $("#username").val().trim();
			        var password = $("#password").val().trim();
			        if (username == "") { $("#msg_div").attr("class", "show"); return } else { $("#msg_div").attr("class", "hide"); }
			        if (password == "") { $("#msg_div1").attr("class", "show"); return } else { $("#msg_div1").attr("class", "hide"); }

			        $.ajax({
			            url: "/Home/btnLogin",
			            type: "POST",
			            data: { playerID: username, password: password },
			            dataType: "json",
			            success: function (result) {
			                if (result.code == 0) {
			                    noty({ text: result.msg, type: "warning", layout: "topCenter", timeout: 1500 });
			                }
			                else {
			                    var returnUrl = getQueryString("ReturnUrl");
			                    if (returnUrl == null) {
			                        window.location.href = "/Home/Index";
			                    }
			                    else {
			                        window.location.href = returnUrl;
			                    }
			                }
			            },
			            error: function () {

			            }
			        });
			    })
			    function getQueryString(name) {
			        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
			        var r = window.location.search.substr(1).match(reg);
			        if (r != null)
			            return unescape(r[2]);
			        return null;
			    }

 </script>
