@{
    Layout = null;
    List<Book.Model.V_Stock_Book> lst = ViewBag.lst;
}
<h2 class="contentTitle">出库录入信息添加：</h2>
<form action="/Stock/Out/AddSave?navTabId=masterList&callbackType=closeCurrent" method="post" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone)">
    <div class="pageContent">
        
       <div class="pageFormContent" layouth="97">
            <dl class="nowrap">
                <dt>出库单据创建时间：</dt>
                <dd>
                    <input id="CreateTime" name="CreateName" value="" class="date"  style="margin-right:400px;"/>
                </dd>
            </dl>
            <dl class="nowrap">
                <dt>关联单据编号：</dt>
                <dd>
                    <input type="text" name="OrderNumber"/>
                </dd>
            </dl>

            <dl class="nowrap">
                <dt>选择客户：</dt>
                <dd>
                    <input type="hidden" name="json.ID"/>
                    <input class="required textInput valid"
                        name="json.Name" type="text"
                        suggestfields="Name"
                        suggesturl="/Stock/Out/getjson"
                        warn="请选择客户"
                        lookupgroup="json" />
                </dd>
            </dl>
            <div class="divider"></div>  
            <div class="divider"></div>
            <h3 class="contentTitle">单据详情</h3>
            <div class="tabs">
                <div class="tabsHeader">
                    <div class="tabsHeaderContent">
                        <ul>
                            <li class="selected"><a href="javascript:void(0)"><span>添加详情</span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="tabsContent" style="height: 260px;">
                    <div>
                        <table class="list nowrap itemDetail" addbutton="添加条目" width="100%">
                            <thead>
                                <tr>
                                    <th type="lookup" name="items[#index#].book.Name"
                                        lookupgroup="items[#index#].book"
                                        @*lookupurl="demo/database/dwzOrgLookup.html"*@
                                        suggesturl="/Stock/Out/getjson2"
                                        suggestfields="Name"
                                        postfield="keywords" size="12" fieldclass="required">书籍名称</th>
                                    <th type="text" name="items[#index#].book.ID" defaultval="#index#" size="12" fieldclass="digits">书籍ID</th>
                                    <th type="text" name="items[#index#].book.Price" defaultval="0.8" size="12" fieldclass="number">价格</th>
                                    <th type="text" name="items[#index#].book.Num" defaultval="#index#" size="12" fieldclass="digits">数量</th>
                                    <th type="text" name="items[#index#].book.Discount" defaultval="0.8" size="12" fieldclass="number">折扣</th>
                                    @*<th type="date" name="items[#index#].itemDate" defaultval="2011-12-28" size="12">日期</th>*@
                                    <th type="del" width="60">操作</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="tabsFooter">
                    <div class="tabsFooterContent"></div>
                </div>
            </div>

        </div>
        <div class="formBar">
            <ul>
                <li>
                    <div class="buttonActive">
                        <div class="buttonContent">
                            <button type="submit">保存</button></div>
                    </div>
                </li>
                <li>
                    <div class="button">
                        <div class="buttonContent">
                            <button class="close" type="button">关闭</button></div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</form>
