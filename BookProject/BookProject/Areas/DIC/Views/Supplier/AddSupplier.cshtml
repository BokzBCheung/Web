@{
    Layout = null;
}

<div class="pageContent">
	<form method="post" action="/Dic/Supplier/AddSupplierSave" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone)">
		<div class="pageFormContent" layoutH="56">
			<p>
				<label>编码：</label>
				<input name="Coding" type="text" size="30" value="" class="required"  alt="请输入编码"/>
			</p>
			<p>
				<label>供应商名称 ：</label>
				<input name="NameEntity" class="required" type="text" size="30" value="" alt="请输入供应商名称"/>
			</p>
            <p>
				<label>供应商地址：</label>
				<input name="AddressEntity" class="required" type="text" size="30" value="" alt=" 请输入供应商地址"/>
			</p>
            <p>
				<label>联系人：</label>
				<input name="Contact" @*class="required"*@ type="text" size="30" value="" alt="请输入联系人"/>
			</p>
            <p>
				<label>联系电话：</label>
				<input name="Phone" @*class="required"*@ type="text" size="30" value="" alt="请输入联系人电话号码"/>
			</p>
			
		</div>
		<div class="formBar">
			<ul>
				<!--<li><a class="buttonActive" href="javascript:;"><span>保存</span></a></li>-->
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li>
					<div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
				</li>
			</ul>
		</div>
	</form>
</div>

